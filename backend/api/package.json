{
    "name": "solana-volume-bot-api",
    "version": "0.1.0",
    "private": true,
    "type": "commonjs",
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
        "build": "tsc -p tsconfig.build.json",
        "start": "node dist/main.js",
        "type-check": "tsc -p tsconfig.build.json --noEmit",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"**/*.{ts,json,md}\"",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "generate-master-key": "ts-node scripts/generate-master-key.ts",
        "rotate-master-key": "ts-node scripts/rotate-master-key.ts",
        "verify-deks": "ts-node scripts/verify-deks.ts",
        "migrate": "node-pg-migrate",
        "migrate:up": "node-pg-migrate up",
        "migrate:down": "node-pg-migrate down",
        "migrate:create": "node-pg-migrate create",
        "migrate:redo": "node-pg-migrate redo",
        "migrate:status": "node-pg-migrate status",
        "db:seed": "ts-node scripts/seed.ts"
    },
    "dependencies": {
        "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
        "@nestjs/common": "^10.4.15",
        "@nestjs/core": "^10.4.15",
        "@nestjs/platform-express": "^10.4.15",
        "@nestjs/platform-socket.io": "^10.4.20",
        "@nestjs/swagger": "^11.2.0",
        "@nestjs/throttler": "^6.4.0",
        "@nestjs/websockets": "^10.4.20",
        "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
        "@solana/spl-token": "^0.4.13",
        "@solana/web3.js": "^1.91.0",
        "@supabase/supabase-js": "^2.48.1",
        "bs58": "^5.0.0",
        "bullmq": "^5.36.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "ioredis": "^5.4.2",
        "jito-ts": "^3.0.1",
        "pino-cloudwatch": "^0.7.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "socket.io": "^4.8.1",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "@nestjs/testing": "^10.4.20",
        "@types/express": "^5.0.3",
        "@types/node": "^24.3.0",
        "@types/node-pg-migrate": "^2.3.1",
        "@vitest/coverage-v8": "^2.1.8",
        "eslint": "^9.16.0",
        "node-pg-migrate": "^8.0.3",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.2",
        "vitest": "^2.1.8"
    }
}
